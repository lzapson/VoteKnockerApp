
<div class="container" id="main-content">

<table class="table table-striped table-light">
  <thead>
    <tr>
      <th scope="col">Date</th>
      <th scope="col">Name</th>
      <th scope="col">Knocked</th>
      <th scope="col">Literature Dropped</th>
      <th scope="col">Petition Signed</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
    </tr>
  </thead>
  <tbody>
    {{#each interactions}}
    <tr>
      <td>{{updatedAt}}</td>
      <td>{{firstName}} {{lastName}}</td>
      <td>
        {{#if knocked}}
          Yes
        {{else}}
          No
        {{/if}}
      </td>
       <td>
        {{#if litDropped}}
          Yes
        {{else}}
          No
        {{/if}}
      </td>
      <td>
        {{#if petitionSigned}}
          Yes
        {{else}}
          No
        {{/if}}
      </td>
      <td>{{email}}</td>
      <td>{{phone}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>


   