
    <!-- Main Content-->
<div class="container" id="main-content">
  <div class="row-col-10">
    <div class="button-center">
      <a href="/interactions/{{status.voterId}}">
        <button type="button" class="btn btn-danger" >Knock This Voter</button>
      </a>
    </div>
  </div>
  <br>
  <div class="jumbotron">
    <!-- Knocking Button --> 
    <div class="row-col-10">
      <div class="flex-table">
          <h3><div id="firstName">{{status.firstName}}</div></h3>
          <h3><div id="middleName">{{status.middleName}}</div></h3>
          <h3><div id="lastName">{{status.lastName}}</div></h3>
      </div>
    </div>
    <div class="row-col-10">
      <div class="flex-table">
          <h4><id class="streetNumber">{{status.streetNum}}</id></h4>
          <h4><id class="streetName">{{status.streetName}}</id></h4>
          <h4><id class="unitNum">{{status.unitNum}}</id></h4>
      </div>
    </div>
    <div class="row-col-10">
      <div class="flex-table">
          <h4><id class="city">{{status.city}},</id></h4>
          <h4><id class="zip">{{status.zip}}</id></h4>
      </div>
    </div>
  {{!-- </div>  <!-- /jumbotron -->  --}}

  {{!-- <div class="jumbotron"> --}}
    <!--     ADD STYLING FOR DISTRICT BOX -->
    <div class="district-box">
      <h3> District Information </h3>
      <br>
      <h4><id class="district">District: {{status.district}}</id></h4>
      <h4><id class="congDistrict">Congressional District: {{status.congDist}}</id></h4>
      <h4><id class="legDistrict">Legislative District: {{status.legDist}}</id></h4>
      <h4><id class="municipality">Municipality: {{status.municipality}}</id></h4>
      <h4><id class="ward">Ward: {{status.ward}}</id></h4>
      <h4><id class="freeholder">Freeholder: {{status.freeholder}}</id></h4>
      <h4><id class="schoolDistrict">School District: {{status.schoolDist}}</id></h4>
      <h4><id class="regionalSchool">Regional School: {{status.regionalSchool}}</id></h4> 
      <h4><id class="party">Party: {{status.party}}</id></h4>
      <h4><id class="status">Status: {{status.status}}</id></h4>  
    </div>
  {{!-- </div> --}}


  {{!-- <div class="jumbotron"> --}}
    <div class="district-box">
      <h2> Voting History</h2>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Election Name</th>
            <th>Type</th>
            <th>Category</th>
            <th>Ballot Type</th>
          </tr>
        </thead>
        <tbody>

    <!--   Voter History  --> 
    {{#each history}}
      <tr>
        <td>{{electionName}}</td>
        <td>{{electionType}}</td>
        <td>{{electionCategory}}</td>
        <td>{{ballotType}}</td>
      </tr>
    {{/each}}
    </tbody>
    </table>
      <!-- Interaction History -->
    {{!-- <div class="row">
      <h2> Interaction History</h2>
    </div> --}}

 <h2> Interaction History</h2>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Date</th>
            <th>Knocked</th>
            <th>Literature Dropped</th>
            <th>Petition Signed</th>
            <th>Email</th>
            <th>Phone</th>
          </tr>
        </thead>
        <tbody>

    <!--   Interaction History  --> 
    {{#each interactions}}
      <tr>
        <td>{{updatedAt}}</td>
        <td>{{knocked}}</td>
        <td>{{litDropped}}</td>
        <td>{{petitionSigned}}</td>
        <td>{{email}}</td>
        <td>{{phone}}</td>
      </tr>
    {{/each}}
    </tbody>
    </table>
   
    </div>
  </div>   <!-- /jumbotron --> 

  <!-- Knocking Button -->
  <div class="row-col-10">
    <div class="button-center">
      <a href="/interactions/{{voterId}}">
        <button type="button" class="btn btn-danger" >Knock This Voter</button>
      </a>
    </div>
  </div>
  <br>

</div>  <!-- /container --> 